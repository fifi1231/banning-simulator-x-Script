function Eventegg()
	local qEventegg = {}
	local args = {
		[1] = "dick",
		[2] = {
			[1] = 1,
			[2] = "500k Event",
			[3] = false
		}
	}
	
	game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("node_modules"):WaitForChild("@rbxts"):WaitForChild("net"):WaitForChild("out"):WaitForChild("_NetManaged"):WaitForChild("eggs/hatchEgg"):FireServer(unpack(args))	
	return qEventegg
end

function hiderplayer()
	local qhiderplayer = {}
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(22716.369140625, 44.495765686035156, 1958.1519775390625)
	return qhiderplayer
end


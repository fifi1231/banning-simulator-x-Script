function AutoEgg()
	local qAutoEgg = {}
	local args={
		[1] = "Egg",
		[2]={
		   [1]=1,
		   [2]=(_G.selectEgg),
		   [3]=true
		}
	}
	
	game:GetService("ReplicatedStorage").rbxts_include.node_modules:FindFirstChild("@rbxts").net.out._NetManaged:FindFirstChild("eggs/hatchEgg"):FireServer(unpack(args))
	return qAutoEgg
end

function Aurorank()
	local qAurorank = {}
	game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("node_modules"):WaitForChild("@rbxts"):WaitForChild("net"):WaitForChild("out"):WaitForChild("_NetManaged"):WaitForChild("unlockRank"):FireServer()
	return qAurorank
end


function reddemchest()
	local qreddemchest = {}
	local args = {
		[1] = "nwm",
		[2] = {}
	}
	
	game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("node_modules"):WaitForChild("@rbxts"):WaitForChild("net"):WaitForChild("out"):WaitForChild("_NetManaged"):WaitForChild("rewards/claimGroupReward"):FireServer(unpack(args))	
	return qreddemchest
end

local player = game.Players.LocalPlayer.Character

function teleport()
	local qteleport = {}
    local farmobject = game:GetService("Workspace").npcs[(_G.selectTOMob)]
	player.HumanoidRootPart.CFrame = farmobject.HumanoidRootPart.CFrame
	return qteleport
end


function Farmmobs()
	local qfarmmobs = {}
	game:GetService("ReplicatedStorage").rbxts_include.node_modules["@rbxts"].net.out._NetManaged.damageNPC:FireServer(workspace.npcs[(_G.selectMob)])
	return qfarmmobs
end


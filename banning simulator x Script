	while task.wait() do
		if getgenv().Trialtp then
			Tteleport()
		end
	end
end)

local playere = game.Players.LocalPlayer.Character

function Tteleport()
	local qTteleport = {}
    local trialnpcs = game:GetService("Workspace").trials.map.npcs[(_G.selecttrialMob)]
	playere.HumanoidRootPart.CFrame = trialnpcs.HumanoidRootPart.CFrame
	return qTteleport
end 

